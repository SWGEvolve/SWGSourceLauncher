<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>swg.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="swg.ico" />
    <None Remove="swg.png" />
    <None Remove="swgbg.jpg" />
    <None Remove="swglaunchbg.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="swg.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\swg.ico" />
    <Resource Include="Assets\swg.png" />
    <Resource Include="Assets\swglaunchbg.png" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Services\" />
  </ItemGroup>

</Project>
